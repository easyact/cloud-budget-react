{
  "consumer": {
    "name": "BudgetWebsite"
  },
  "provider": {
    "name": "BudgetApiGateway"
  },
  "interactions": [
    {
      "description": "first upload",
      "providerState": "no user",
      "request": {
        "method": "POST",
        "path": "/v0/users/damoco@easyact.cn/events",
        "headers": {
          "Content-Type": "application/json"
        },
        "body": [
          {
            "type": "IMPORT_BUDGET",
            "user": {
              "email": "damoco@easyact.cn"
            },
            "to": {
              "version": "0"
            },
            "payload": {
              "assets": [
                {
                  "id": "",
                  "name": "food",
                  "amount": 10
                }
              ]
            }
          }
        ],
        "matchingRules": {
          "$.body": {
            "min": 1
          },
          "$.body[*].*": {
            "match": "type"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "access-control-allow-origin": "*"
        },
        "body": {
        }
      }
    },
    {
      "description": "new client get",
      "providerState": "damoco imported",
      "request": {
        "method": "GET",
        "path": "/v0/users/damoco@easyact.cn/events"
      },
      "response": {
        "status": 200,
        "headers": {
          "access-control-allow-origin": "*"
        },
        "body": [
          {
            "type": "IMPORT_BUDGET",
            "user": {
              "email": "damoco@easyact.cn"
            },
            "to": {
              "version": "0"
            },
            "payload": {
              "assets": [
                {
                  "id": "",
                  "name": "food",
                  "amount": 10
                }
              ]
            },
            "at": "2021-05-14T00:00:00.012+08",
            "user.email": "damoco@easyact.cn"
          }
        ],
        "matchingRules": {
          "$.body[0].at": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}